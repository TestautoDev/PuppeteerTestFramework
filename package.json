{
  "name": "Puppeteer-ui-framework",
  "version": "10.0.0",
  "description": "Puppeteer test framework build on top of chrome dev protocol",
  "main": "jest",
  "scripts": {
    "test:ci": "cross-env NODE_ENV=beteasy.com.au jest src/test/MockTest.test --runInBand",
    "test": "cross-env NODE_ENV=beteasy.com.au jest --runInBand",
    "test:local": "cross-env NODE_ENV=beteasy.com.au jest Logintest.test.js --runInBand",
    "watch": "cross-env NODE_ENV=beteasy.com.au jest --watch",
    "coverage": "cross-env NODE_ENV=beteasy.com.au jest --runInBand --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "report": "allure generate"
  },
  "author": "Dipjyoti Metia",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@types/puppeteer": "3.0.1",
    "allure-commandline": "2.13.0",
    "async": "3.2.0",
    "aws-sdk": "2.712.0",
    "axios": "0.19.2",
    "chai": "4.2.0",
    "chalk": "^4.1.0",
    "cross-env": "7.0.2",
    "date-fns": "2.14.0",
    "expect-puppeteer": "^4.4.0",
    "faker": "^4.1.0",
    "fse": "4.0.1",
    "fs-extra": "9.0.1",
    "jest": "^26.1.0",
    "jest-allure": "0.1.3",
    "jest-environment-node": "^26.1.0",
    "jest-junit": "11.0.1",
    "jest-puppeteer": "^4.4.0",
    "lodash": "4.17.15",
    "mkdirp": "1.0.4",
    "pptr-mock-server": "^1.0.17",
    "puppeteer": "^5.0.0",
    "recursive-readdir": "2.2.2",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.0",
    "winston": "^3.3.3",
    "yaml": "1.10.0",
    "localforage": "1.7.4"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "babel-eslint": "^10.1.0",
    "eslint": "7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.6",
    "husky": "^4.2.5",
    "jest-html-reporters": "^2.0.1",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "puppeteer-har": "^1.1.2"
  }
}
