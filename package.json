{
  "name": "Puppeteer-ui-framework",
  "version": "8.0.0",
  "description": "Puppeteer test framework build on top of chrome dev protocol",
  "main": "jest",
  "scripts": {
    "test:ci": "cross-env NODE_ENV=beteasy.com.au BROWSER=firefox jest src/test/Logintest.test --runInBand",
    "test": "cross-env NODE_ENV=beteasy.com.au jest --runInBand",
    "test:local": "cross-env NODE_ENV=beteasy.com.au BROWSER=chromium jest Registration.test.js --runInBand",
    "watch": "cross-env NODE_ENV=beteasy.com.au jest --watch",
    "coverage": "cross-env NODE_ENV=beteasy.com.au jest --runInBand --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "report": "allure generate"
  },
  "author": "Dipjyoti Metia",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "allure-commandline": "2.13.0",
    "async": "3.2.0",
    "aws-sdk": "2.657.0",
    "axios": "0.19.2",
    "chai": "4.2.0",
    "chalk": "^4.0.0",
    "cross-env": "7.0.2",
    "date-fns": "2.12.0",
    "faker": "^4.1.0",
    "fse": "4.0.1",
    "fs-extra": "9.0.0",
    "jest": "^26.0.1",
    "jest-allure": "0.1.1",
    "jest-environment-node": "^25.5.0",
    "jest-junit": "10.0.0",
    "lodash": "4.17.15",
    "mkdirp": "0.5.1",
    "playwright": "1.0.2",
    "jest-playwright-preset": "0.1.3",
    "recursive-readdir": "2.2.2",
    "rimraf": "^3.0.2",
    "sqlite3": "^4.2.0",
    "winston": "^3.2.1",
    "yaml": "1.8.3",
    "localforage": "1.7.3"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "babel-eslint": "^10.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "jest-html-reporters": "^1.2.1",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5"
  }
}
