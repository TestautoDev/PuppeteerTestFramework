{
  "name": "PlaywrightTestframework",
  "version": "1.0.0",
  "description": "Puppeteer test framework build on top of chrome dev protocol",
  "main": "jest",
  "scripts": {
    "test:ci": "cross-env NODE_ENV=beteasy.com.au BROWSER=firefox jest src/test/Logintest.test --runInBand",
    "test": "cross-env NODE_ENV=beteasy.com.au jest --runInBand",
    "test:local": "cross-env NODE_ENV=beteasy.com.au BROWSER=chromium jest Registration.test.js --runInBand",
    "watch": "cross-env NODE_ENV=beteasy.com.au jest --watch",
    "coverage": "cross-env NODE_ENV=beteasy.com.au jest --runInBand --coverage",
    "report": "allure generate"
  },
  "author": "Dipjyoti Metia",
  "license": "ISC",
  "dependencies": {
    "allure-commandline": "2.13.0",
    "async": "3.2.0",
    "aws-sdk": "2.738.0",
    "axios": "0.20.0",
    "chai": "4.2.0",
    "chalk": "^4.1.0",
    "cross-env": "7.0.2",
    "date-fns": "2.15.0",
    "faker": "^4.1.0",
    "fse": "4.0.1",
    "fs-extra": "9.0.1",
    "jest-allure": "0.1.3",
    "jest-environment-node": "^26.3.0",
    "jest-junit": "11.1.0",
    "lodash": "4.17.20",
    "mkdirp": "1.0.4",
    "recursive-readdir": "2.2.2",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.0",
    "winston": "^3.3.3",
    "yaml": "1.10.0",
    "localforage": "1.9.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.4",
    "@babel/preset-env": "7.11.0",
    "jest": "^26.4.2",
    "jest-html-reporters": "^2.0.3",
    "jest-playwright-preset": "^1.3.1",
    "playwright": "^1.3.0",
    "prettier": "^2.1.1"
  }
}
