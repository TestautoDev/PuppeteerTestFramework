{
  "name": "Puppeteer-ui-framework",
  "version": "5.0.0",
  "description": "Puppeteer test framework build on top of chrome dev protocol",
  "main": "jest",
  "scripts": {
    "test": "cross-env NODE_ENV=beteasy.com.au jest --runInBand",
    "watch": "cross-env NODE_ENV=beteasy.com.au jest --watch",
    "coverage": "cross-env NODE_ENV=beteasy.com.au jest --runInBand --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "report": "allure generate"
  },
  "author": "Dipjyoti Metia",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "allure-commandline": "2.9.0",
    "chalk": "^2.4.2",
    "cross-env": "5.2.0",
    "faker": "^4.1.0",
    "jest": "^24.8.0",
    "axios": "0.18.0",
    "chai": "4.2.0",
    "jest-junit": "6.4.0",
    "jest-allure": "0.1.1",
    "lodash": "4.17.11",
    "mkdirp": "0.5.1",
    "puppeteer": "^1.17.0",
    "rimraf": "^2.6.3",
    "winston": "^3.2.1",
    "yaml": "1.6.0",
    "fse": "4.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "husky": "^2.3.0",
    "jest-environment-node": "^24.8.0",
    "lint-staged": "^8.1.7",
    "puppeteer-har": "^1.0.6",
    "babel-eslint": "^10.0.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "prettier": "^1.17.1"
  }
}
