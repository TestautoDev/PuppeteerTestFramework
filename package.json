{
  "name": "Puppeteer-ui-framework",
  "version": "8.0.0",
  "description": "Puppeteer test framework build on top of chrome dev protocol",
  "main": "jest",
  "scripts": {
    "test:ci": "cross-env NODE_ENV=beteasy.com.au jest src/test/MockTest.test --runInBand",
    "test": "cross-env NODE_ENV=beteasy.com.au jest --runInBand",
    "test:local": "cross-env NODE_ENV=beteasy.com.au jest Logintest.test.js --runInBand",
    "watch": "cross-env NODE_ENV=beteasy.com.au jest --watch",
    "coverage": "cross-env NODE_ENV=beteasy.com.au jest --runInBand --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "report": "allure generate"
  },
  "author": "Dipjyoti Metia",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "allure-commandline": "2.13.0",
    "async": "3.1.0",
    "aws-sdk": "2.556.0",
    "axios": "0.19.2",
    "chai": "4.2.0",
    "chalk": "^3.0.0",
    "cross-env": "6.0.3",
    "date-fns": "2.9.0",
    "expect-puppeteer": "^4.4.0",
    "faker": "^4.1.0",
    "fse": "4.0.1",
    "fs-extra": "8.1.0",
    "jest": "^25.1.0",
    "jest-allure": "0.1.1",
    "jest-environment-node": "^25.1.0",
    "jest-junit": "10.0.0",
    "jest-puppeteer": "^4.4.0",
    "lodash": "4.17.15",
    "mkdirp": "0.5.1",
    "pptr-mock-server": "^1.0.5",
    "puppeteer": "^2.1.0",
    "recursive-readdir": "2.2.2",
    "rimraf": "^3.0.0",
    "sqlite3": "^4.1.1",
    "winston": "^3.2.1",
    "yaml": "1.7.2",
    "localforage": "1.7.3"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "@percy/puppeteer": "^1.0.8",
    "babel-eslint": "^10.0.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^4.2.1",
    "jest-html-reporters": "^1.2.1",
    "lint-staged": "^10.0.3",
    "prettier": "^1.19.1",
    "puppeteer-har": "^1.1.2"
  }
}
