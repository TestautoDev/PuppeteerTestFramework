{
  "name": "Puppeteer-ui-framework",
  "version": "4.0.0",
  "description": "Puppeteer test framework build on top of chrome dev protocol",
  "main": "jest",
  "scripts": {
    "test": "cross-env NODE_ENV=beteasy.com.au jest --runInBand",
    "posttest": "allure generate",
    "watch": "cross-env NODE_ENV=beteasy.com.au jest --watch",
    "coverage": "cross-env NODE_ENV=beteasy.com.au jest --runInBand --coverage",
    "lint": "npm run jslint \"test/*.js\""
  },
  "author": "Dipjyoti Metia",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "allure-commandline": "2.9.0",
    "chalk": "^2.4.2",
    "cross-env": "5.2.0",
    "eslint": "5.14.1",
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "jest-allure": "0.1.1",
    "lodash": "4.17.11",
    "mkdirp": "0.5.1",
    "puppeteer": "^1.12.2",
    "rimraf": "^2.6.3",
    "winston": "^3.2.1",
    "yaml": "1.3.2",
    "fse": "4.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "husky": "^1.3.1",
    "jest-environment-node": "^24.0.0",
    "lint-staged": "^8.1.4",
    "puppeteer-har": "^1.0.5"
  }
}